apiVersion: kubeadm.k8s.io/v1beta4
kind: ClusterConfiguration
kubernetesVersion: stable
apiServer:
  certSANs:
  - "10.10.0.100"
  - "10.10.0.20"
controlPlaneEndpoint: "10.10.0.100:6443"
networking:
  podSubnet: 172.16.1.0/16
etcd:
  external:
    endpoints:
    - https://10.10.0.10:2379
    caFile: /etc/kubernetes/pki/etcd/ca.crt
    certFile: /etc/kubernetes/pki/etcd/kube-etcd-client.crt
    keyFile: /etc/kubernetes/pki/etcd/kube-etcd-client.key